#include<iostream>
using namespace std;
void Sumofarray(int arr[],int n){
    int sum=0;
    for(int i=0;i<=n;i++){
        int term=arr[i];
        sum=term+sum;
    }
    cout<<"Sum of number in the array="<<sum;
}
int main(){
    int numbers[]={5,8,6,-5,18};
    int size=sizeof(numbers)/sizeof(int);
    Sumofarray(numbers,size);

}
