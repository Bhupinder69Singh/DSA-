
#include<iostream>
using namespace std;
void Printarray(int arr[],int size){
    cout<<"Printing the array"<<endl;
    for(int i=0;i<=size;i++){
        int term=arr[i];
        cout<<" "<<term;
    }
    cout<<endl<<"Done printing the array"<<endl;
};
int main(){
    int first[]={1,2,3,4,5,6};
    int x=sizeof(first)/sizeof(int);
    Printarray(first,x);
}
